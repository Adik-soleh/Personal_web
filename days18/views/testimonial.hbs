  <link rel="icon"
    href="https://w7.pngwing.com/pngs/16/42/png-transparent-icon-cool-miscellaneous-emoticon-apple-icon-image-format.png"
    type="image/x-icon" />
  <!-- bootstrap -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet"
