<body>
  <!-- navbar bootstrap-->

  <nav class="navbar navbar-expand-lg bg-success text-white fixed-top">
    <div class="container-fluid">
      <a class="navbar-brand" href="/">
        <img src="https://cdn.pixabay.com/photo/2016/08/25/07/30/red-1618916_640.png" width="50px" alt="Brand Logo">
      </a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarTogglerDemo02"
        aria-controls="navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarTogglerDemo02">
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item">
            <a class="nav-link text-white" aria-current="page" href="/">Home</a>
          </li>
          <li class="nav-item">
            <a class="nav-link text-white" href="/project">Project</a>
          </li>
          <li class="nav-item">
            <a class="nav-link text-white btn btn-outline-warning" href="testimonial">Testimoni</a>
          </li>
        </ul>
        {{#if user}}
        <div class="d-flex align-items-center">
          <a href="contac" class="btn btn-dark text-white mx-2" type="button">Contact Us</a>
        </div>
        {{/if}}
      </div>
    </div>
  </nav>
  <!-- end navbar -->
  <!-- end navbar -->
  <!-- Testimonial -->
  <section class="pt-5">
    <h1>MY Testimonials</h1>
    <hr>
    <div class="Star-Rate text-center">
      <h3>Ratting By Star</h3>

      <button>
        <i class="fa fa-star" aria-hidden="true" onclick="allRate()">All</i>
      </button>
      <button>
        <i class="fa fa-star" aria-hidden="true" onclick="StarRating(1)">1</i>
      </button>
      <button>
        <i class="fa fa-star" aria-hidden="true" onclick="StarRating(2)">2</i>
      </button>
      <button>
        <i class="fa fa-star" aria-hidden="true" onclick="StarRating(3)">3</i>
      </button>
      <button>
        <i class="fa fa-star" aria-hidden="true" onclick="StarRating(4)">4</i>
      </button>
      <button>
        <i class="fa fa-star" aria-hidden="true" onclick="StarRating(5)">5</i>
      </button>
    </div>

    {{!-- card --}}
    <div class="container-cards" id="container-card" style="text-align: center;">
    </div>
     {{!-- End card --}}
     
  </section>
  <!-- End Testimonial -->


  <script src="assets/js/ajax.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"
