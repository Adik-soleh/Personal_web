  {{!-- start card --}}
  <h1 class="text-center text-white">MY PROJECT</h1>
  <div class="container mt-4">
    <div class="container container-cards">
      <div class="row">
        {{#each blog}}
        <div class="col-md-4 mb-3">
          <div class="card">
            <img src="{{image}}" alt="...">
            <div class="card-body">
              <h5 class="card-title text-black">{{title}}</h5>
              <p class="card-text text-black">{{content}}</p>
              <div class="row row-cols-auto">
                {{#if checkBox1}} <div class="col shadow p-2 mb-4  rounded bg-white " style="margin-right: 7px;"> <i
                    class="fa-brands fa-java fa-2x"></i></div>{{/if}}
                {{#if checkBox2}} <div class="col shadow p-2 mb-4  rounded bg-white" style="margin-right: 7px;"><i
                    class="fa-brands fa-js fa-2x"></i></div>{{/if}}
                {{#if checkBox3}}<div class="col shadow p-2 mb-4  rounded bg-white" style="margin-right: 7px;"><i
                    class="fa-brands fa-python fa-2x"></i></div>{{/if}}
                {{#if checkBox4}}<div class="col shadow p-2 mb-4  rounded bg-white" style="margin-right: 7px;"><i
                    class="fa-brands fa-golang fa-2x"></i></div>{{/if}}
              </div>
            </div>
            <div class="d-flex justify-content-between update">
              <a href="#" class="btn btn-dark">Update</a>
              <a href="#" class="btn btn-danger">Delete</a>
            </div>
          </div>
        </div>
        {{/each}}
